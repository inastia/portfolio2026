{
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/about",
      "destination": "/src/about.html"
    },
    {
      "source": "/contact",
      "destination": "/src/contact.html"
    },
    {
      "source": "/project/:name",
      "destination": "/src/projects/:name"
    }
  ]
}
